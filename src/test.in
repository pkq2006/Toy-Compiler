int main()
{
    int i;
    int j;
    int k;
    int l;
    int m;
    int a = 99;
    int n = 5;
    int cnt = 0;
    for (i = 1; i <= n; i ++)
        for (j = 1; j <= n; j ++)
            for (k = 1; k <= n; k ++)
                for (l = 1; l <= n; l ++)
                    for (m = 1; m <= n; m ++)
                    if
                    (
                    i != j && i != k && i != l && i != m && i != a &&
                    j != k && j != l && j != m && j != a &&
                    k != l && k != m && k != a &&
                    l != m && l != a &&
                    m != a
                    )
                        cnt ++;
    return cnt;
}